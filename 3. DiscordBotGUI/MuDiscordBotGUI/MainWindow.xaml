                <TextBox x:Name="tablesMembers" materialDesign:HintAssist.Hint="Members" Style="{StaticResource MaterialDesignFloatingHintTextBox}" Foreground="White" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="50,70,0,0" MaxWidth="170" Width="170" MinWidth="170" />
                <TextBox x:Name="tablesStats" materialDesign:HintAssist.Hint="Stats" Style="{StaticResource MaterialDesignFloatingHintTextBox}" Foreground="White" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="50,120,0,0" MaxWidth="170" Width="170" MinWidth="170" />
