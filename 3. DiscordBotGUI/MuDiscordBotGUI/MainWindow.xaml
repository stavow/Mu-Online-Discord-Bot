                <TextBox x:Name="sqlDb" materialDesign:HintAssist.Hint="Database" Style="{StaticResource MaterialDesignFloatingHintTextBox}" Foreground="White" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="50,70,0,0" MaxWidth="170" Width="170" MinWidth="170" />
