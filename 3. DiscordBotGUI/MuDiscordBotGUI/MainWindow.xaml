<Window x:Class="MuDiscordBotGUI.MainWindow"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MuDiscordBotGUI"
        mc:Ignorable="d"
        AllowsTransparency="True"
        ResizeMode="CanResize"
        WindowStyle="None"
        BorderThickness="1"
        BorderBrush="Black"
        Title="MainWindow" Height="600" Width="800" MinWidth="800" MinHeight="600" Background="#FF111111" Closing="Window_Closing">
    <Window.Resources>
        <Style x:Key="RectBorderStyle" TargetType="Rectangle">
            <Setter Property="Focusable" Value="False" />
            <Setter Property="Fill" Value="Transparent" />
            <Setter Property="Tag" Value="{Binding RelativeSource={RelativeSource AncestorType=Window}}" />
            <EventSetter Event="MouseLeftButtonDown" Handler="Resize_Init"/>
            <EventSetter Event="MouseLeftButtonUp" Handler="Resize_End"/>
            <EventSetter Event="MouseMove" Handler="Resizeing_Form"/>
        </Style>
        <Style x:Key="CustomMaterialDesignFlatButton" 
        BasedOn="{StaticResource MaterialDesignFlatButton}"
        TargetType="{x:Type Button}">
            <Style.Resources>
                <Border x:Key="PrimaryBorderBrush" CornerRadius="0,0,0,0"></Border>
                <Color x:Key="PrimaryHueMidBrushColor">Red</Color>
                <SolidColorBrush x:Key="PrimaryHueMidBrush" Color="White" />

            </Style.Resources>
        </Style>
        <Storyboard x:Key="OpenMenu">
            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(FrameworkElement.Width)" Storyboard.TargetName="GridMenu">
                <EasingDoubleKeyFrame KeyTime="0" Value="50"/>
                <EasingDoubleKeyFrame KeyTime="0:0:0.2" Value="200"/>
            </DoubleAnimationUsingKeyFrames>
        </Storyboard>
        <Storyboard x:Key="CloseMenu">
            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(FrameworkElement.Width)" Storyboard.TargetName="GridMenu">
                <EasingDoubleKeyFrame KeyTime="0" Value="200"/>
                <EasingDoubleKeyFrame KeyTime="0:0:0.2" Value="50"/>
            </DoubleAnimationUsingKeyFrames>
        </Storyboard>
    </Window.Resources>
    <Window.Triggers>
        <EventTrigger RoutedEvent="ButtonBase.Click" SourceName="ButtonOpenMenu">
            <BeginStoryboard Storyboard="{StaticResource OpenMenu}"/>
        </EventTrigger>
        <EventTrigger RoutedEvent="ButtonBase.Click" SourceName="ButtonCloseMenu">
            <BeginStoryboard Storyboard="{StaticResource CloseMenu}"/>
        </EventTrigger>
    </Window.Triggers>
    <Grid>

        <Grid Name="consoleGrid" Margin="50,77,0,0">
            <Button x:Name="stopBot" Style="{StaticResource MaterialDesignIconButton}" Foreground="#FF0BC8FF" Background="{DynamicResource MaterialDesignTextFieldBoxBackground}" Margin="0,15,16,0" HorizontalAlignment="Right" VerticalAlignment="Top" PreviewMouseLeftButtonDown="stopBot_MouseLeftButtonDown">
                <materialDesign:PackIcon Kind="Stop" />
            </Button>
            <Button x:Name="startBot" Style="{StaticResource MaterialDesignIconButton}" Foreground="#FF0BC8FF" Background="{DynamicResource MaterialDesignTextFieldBoxBackground}" Margin="0,15,76,0" HorizontalAlignment="Right" VerticalAlignment="Top" PreviewMouseLeftButtonDown="startBot_MouseLeftButtonDown">
                <materialDesign:PackIcon Kind="Play"/>
            </Button>
            <GroupBox Header="C O N S O L E"  Margin="16,80, 16,16"   materialDesign:ColorZoneAssist.Mode="Custom"    materialDesign:ColorZoneAssist.Foreground="Black" materialDesign:ColorZoneAssist.Background="#FF0BC8FF" FontFamily="./Fonts/#Poppins Regular">
                <TextBox x:Name="console" Foreground="White" FontFamily="Consolas" IsReadOnly="True" />
            </GroupBox>
        </Grid>


        <Grid Name="settingsGrid" Margin="50,77,0,0" Visibility="Hidden">
            <Grid Margin="0,0,0,400" VerticalAlignment="Top" Height="121">
                <ComboBox x:Name="categ" Foreground="White" Style="{StaticResource MaterialDesignFilledComboBox}" HorizontalAlignment="Center" MaxWidth="300" materialDesign:HintAssist.Hint="Category" Margin="0,31,0,31" MinWidth="180" Height="54">
                    <ComboBoxItem Content="SQL" />
                    <ComboBoxItem Content="Tables" />
                    <ComboBoxItem Content="Members" />
                    <ComboBoxItem Content="Stats" />
                    <ComboBoxItem Content="Accounts" />
                    <ComboBoxItem Content="Character" />
                    <ComboBoxItem Content="Information" />
                    <ComboBoxItem Content="Discord" />
                    <ComboBoxItem Content="Discord Events" />
                    <ComboBoxItem Content="Discord Shop" />
                    <ComboBoxItem Content="Discord Emojis" />
                    <ComboBoxItem Content="Disable Commands" />
                </ComboBox>
                <Button x:Name="choose" Style="{StaticResource MaterialDesignIconButton}" Foreground="#FF0BC8FF" Background="{DynamicResource MaterialDesignTextFieldBoxBackground}" Margin="270,36,0,31" HorizontalAlignment="Center" VerticalAlignment="Top" PreviewMouseLeftButtonDown="choose_MouseLeftButtonDown">
                    <materialDesign:PackIcon Kind="CheckBold" PreviewMouseLeftButtonDown="choose_MouseLeftButtonDown"/>
                </Button>
                <Button x:Name="save" Style="{StaticResource MaterialDesignIconButton}" Foreground="#FF0BC8FF" Background="{DynamicResource MaterialDesignTextFieldBoxBackground}" Margin="390,36,0,31" HorizontalAlignment="Center" VerticalAlignment="Top" PreviewMouseLeftButtonDown="save_MouseLeftButtonDown">
                    <materialDesign:PackIcon Kind="ContentSave" PreviewMouseLeftButtonDown="save_MouseLeftButtonDown"/>
                </Button>
                <Button x:Name="undo" Style="{StaticResource MaterialDesignIconButton}" Foreground="#FF0BC8FF" Background="{DynamicResource MaterialDesignTextFieldBoxBackground}" Margin="510,36,0,31" HorizontalAlignment="Center" VerticalAlignment="Top" PreviewMouseLeftButtonDown="undo_MouseLeftButtonDown">
                    <materialDesign:PackIcon Kind="UndoVariant" PreviewMouseLeftButtonDown="undo_MouseLeftButtonDown"/>
                </Button>
            </Grid>
            <Grid x:Name="sqlConfig" Margin="0,121,0,0" Visibility="Hidden">
