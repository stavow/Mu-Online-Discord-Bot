                <TextBox x:Name="discordHighModerationRoles" materialDesign:HintAssist.Hint="High Moderation Roles" Style="{StaticResource MaterialDesignFloatingHintTextBox}" Foreground="White" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="270,20,0,0" MaxWidth="170" Width="170" MinWidth="170" />
