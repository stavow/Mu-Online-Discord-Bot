                <TextBox x:Name="command" materialDesign:HintAssist.Hint="Command" Style="{StaticResource MaterialDesignFloatingHintTextBox}" Foreground="White" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="270,70,0,0" MaxWidth="170" Width="170" MinWidth="170" />
                <TextBox x:Name="levelUpPoints" materialDesign:HintAssist.Hint="LevelUpPoints" Style="{StaticResource MaterialDesignFloatingHintTextBox}" Foreground="White" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="270,120,0,0" MaxWidth="170" Width="170" MinWidth="170" />
                <TextBox x:Name="inventory" materialDesign:HintAssist.Hint="Inventory" Style="{StaticResource MaterialDesignFloatingHintTextBox}" Foreground="White" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="270,170,0,0" MaxWidth="170" Width="170" MinWidth="170" />
