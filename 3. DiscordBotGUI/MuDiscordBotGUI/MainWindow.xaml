                <TextBox x:Name="maxLife" materialDesign:HintAssist.Hint="Max Life" Style="{StaticResource MaterialDesignFloatingHintTextBox}" Foreground="White" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="270,270,0,0" MaxWidth="170" Width="170" MinWidth="170" />
                <TextBox x:Name="mapNumber" materialDesign:HintAssist.Hint="Map Number" Style="{StaticResource MaterialDesignFloatingHintTextBox}" Foreground="White" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="270,320,0,0" MaxWidth="170" Width="170" MinWidth="170" />

                <TextBox x:Name="mapX" materialDesign:HintAssist.Hint="Map X" Style="{StaticResource MaterialDesignFloatingHintTextBox}" Foreground="White" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="490,20,0,0" MaxWidth="80" Width="80" MinWidth="80" />
                <TextBox x:Name="mapY" materialDesign:HintAssist.Hint="Map Y" Style="{StaticResource MaterialDesignFloatingHintTextBox}" Foreground="White" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="580,20,0,0" MaxWidth="80" Width="80" MinWidth="80" />

                <TextBox x:Name="PkLevel" materialDesign:HintAssist.Hint="Pk Level" Style="{StaticResource MaterialDesignFloatingHintTextBox}" Foreground="White" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="490,70,0,0" MaxWidth="80" Width="80" MinWidth="80" />
                <TextBox x:Name="PkCount" materialDesign:HintAssist.Hint="Pk Count" Style="{StaticResource MaterialDesignFloatingHintTextBox}" Foreground="White" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="580,70,0,0" MaxWidth="80" Width="80" MinWidth="80" />

                <TextBox x:Name="ctlCode" materialDesign:HintAssist.Hint="Ctl Code" Style="{StaticResource MaterialDesignFloatingHintTextBox}" Foreground="White" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="490,120,0,0" MaxWidth="170" Width="170" MinWidth="170" />
                <TextBox x:Name="resets" materialDesign:HintAssist.Hint="Resets" Style="{StaticResource MaterialDesignFloatingHintTextBox}" Foreground="White" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="490,170,0,0" MaxWidth="170" Width="170" MinWidth="170" />
