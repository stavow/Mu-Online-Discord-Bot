                <TextBox x:Name="communicationChannel" materialDesign:HintAssist.Hint="Communication Channel" Style="{StaticResource MaterialDesignFloatingHintTextBox}" Foreground="White" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="50,170,0,0" MaxWidth="170" Width="170" MinWidth="170" />
                <TextBox x:Name="levelupChannel" materialDesign:HintAssist.Hint="Levelup Channel" Style="{StaticResource MaterialDesignFloatingHintTextBox}" Foreground="White" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="50,220,0,0" MaxWidth="170" Width="170" MinWidth="170" />
                <TextBox x:Name="spamChannel" materialDesign:HintAssist.Hint="Spam Channel" Style="{StaticResource MaterialDesignFloatingHintTextBox}" Foreground="White" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="50,270,0,0" MaxWidth="170" Width="170" MinWidth="170" />
